function secondSelection() {
    // Get the selected value from the first drop-down menu
    var pipe_mat_value = document.getElementById("pipe_mat_HTML").value;


    // Get a reference to the second drop-down menu
    var pipe_size = document.getElementById("pipe_size_HTML");

    // Remove all existing options from the third drop-down menu
    var pipe_WT = document.getElementById("pipe_WT_HTML");

    // Remove all existing options from the third drop-down menu
    pipe_WT.innerHTML = "";

    // Remove all existing options from the second drop-down menu
    pipe_size.innerHTML = "";

    // Adds Select an option to pipe size, then disables it so it cannot be selected again
    var option = new Option("Select an option", "pipe_size_value");
    option.disabled = true;
    option.selected = true;
    pipe_size.add(option);

    // Takes the elements that are accoicated with the pipe material and puts them in the second dropdown menu
    for (const key in lookupTable[pipe_mat_value]) {
        pipe_size.add(new Option(key, "pipe_size_value"));
    }
}

function thirdSelection() {
    // Get the selected value from the first drop-down menu
    var pipe_mat_value = document.getElementById("pipe_mat_HTML").value;

    // Get a reference to the second drop-down menu
    var pipe_size = document.getElementById("pipe_size_HTML");
    var pipe_size_value = pipe_size.options[pipe_size.selectedIndex].text;

    // Get a reference to the third drop-down menu
    var pipe_WT = document.getElementById("pipe_WT_HTML");

    // Remove all existing options from the second drop-down menu
    pipe_WT.innerHTML = "";

    // An array of Wall thickness's that are associated with the pipe size and pipe material
    var WT_array = lookupTable[pipe_mat_value][pipe_size_value];

    // Adds Select an option to Wall thickness, then disables it
    var option = new Option("Select an option", "pipe_WT_value");
    option.disabled = true;
    option.selected = true;
    pipe_WT.add(option);

    // Takes the array of elements and puts them in the third dropdown menu
    WT_array.forEach(function(element) {
        pipe_WT.add(new Option(element, "pipe_WT_value"));
    });

}

function printValues() {
    var pipe_size = document.getElementById("pipe_size_HTML");
    var pipe_size_value = pipe_size.options[pipe_size.selectedIndex].text;

    var pipe_WT = document.getElementById("pipe_WT_HTML");
    var pipe_WT_value = pipe_WT.options[pipe_WT.selectedIndex].text;

    var pipe_mat_value = document.getElementById("pipe_mat_HTML").value;

    document.getElementById("pipe_mat").innerHTML = pipe_mat_value;
    document.getElementById("pipe_size").innerHTML = pipe_size_value;
    document.getElementById("pipe_WT").innerHTML = pipe_WT_value;

}
